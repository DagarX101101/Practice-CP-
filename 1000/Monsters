
#include <bits/stdc++.h>
using namespace std;
struct cmp
{
    bool operator()(const pair<int, int> &a,
                    const pair<int, int> &b)
    {
        if (a.first != b.first)
            return a.first < b.first; // smaller first â†’ min-heap by first
        return a.second > b.second;   // larger second first
    }
};
void solve()
{
    int n;
    cin >> n;
    int k;
    cin >> k;
    vector<int> vec;
    for (int i = 0; i < n; i++)
    {
        int a;
        cin >> a;
        vec.push_back(a);
    }
    priority_queue<
        pair<int, int>,
        vector<pair<int, int>>,
        cmp>
        pq;
    vector<int> ans;
    for (int i = 0; i < n; i++)
    {
        vec[i] = vec[i] % k;
        if (vec[i] == 0)
        {
            ans.push_back(i);
        }
        pq.push({vec[i], i});
    }
    while (!pq.empty())
    {
        ans.push_back(pq.top().second);
        pq.pop();
    }
    for (int i = 0; i < n; i++)
    {
        cout << ans[i] + 1 << " ";
    }
    cout << endl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--)
        solve();

    return 0;
}
